<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Vision Mobile</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>vision_mobile</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.134229584480-7db0836a7c3b12689cb105</string>
			</array>
		</dict>
	</array>
	
	<!-- Privacy - Camera Usage Description -->
	<key>NSCameraUsageDescription</key>
	<string>WayFinder needs camera access to identify objects in front of you and help with navigation.</string>
	
	<!-- Privacy - Microphone Usage Description -->
	<key>NSMicrophoneUsageDescription</key>
	<string>WayFinder needs microphone access to listen for your voice commands and the 'Hey WayFinder' wake word.</string>
	
	<!-- Privacy - Location When In Use Usage Description -->
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>WayFinder uses your location to provide accurate turn-by-turn navigation and build routes.</string>
	
	<!-- Privacy - Location Always Usage Description -->
	<key>NSLocationAlwaysUsageDescription</key>
	<string>WayFinder requires background location to continue guiding you even when your phone is in your pocket.</string>
	
	<!-- Privacy - Speech Recognition Usage Description -->
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>WayFinder uses speech recognition to convert your voice into text commands for the AI.</string>

	<!-- Privacy - Photo Library Usage Description -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>WayFinder may need access to your photos if you want to analyze saved images.</string>

	<!-- Required for Background Modes -->
	<key>UIBackgroundModes</key>
	<array>
		<string>audio</string>
		<string>location</string>
		<string>fetch</string>
		<string>remote-notification</string>
	</array>

	<!-- Performance and UI -->
	<key>io.flutter.embedded_views_preview</key>
	<true/>
</dict>
</plist>
